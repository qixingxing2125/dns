<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iie.manage.mapper.SysLabelMapper">
	<select id="findDomainLabel" resultType="com.iie.manage.pojo.SysLabel">
		select * from sys_labels
	</select>
	
	<select id="queryLabelName" resultType="string">
		select name from sys_labels where id=#{labelId}
	</select>
	
	<select id="findLabelId" resultType="int">
		select id from sys_labels where name=#{label}
	</select>
</mapper>



